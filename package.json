{
  "name": "ackee-frontend",
  "displayName": "Ackee frontend",
  "description": "A set of usefull snippets and commands we use at Ackee for web apps development.",
  "version": "0.1.2",
  "repository": "https://github.com/AckeeCZ/vscode-frontend",
  "publisher": "ackee",
  "license": "MIT",
  "icon": "images/ackee-logo.png",
  "galleryBanner": {
    "color": "#000000",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.36.0"
  },
  "categories": [
    "Snippets",
    "Other"
  ],
  "main": "./src/extension.js",
  "activationEvents": [
    "onCommand:extension.createModule"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.createModule",
        "title": "Create module"
      }
    ],
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/javascriptreact.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "tslint -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.36.0",
    "glob": "^7.1.4",
    "typescript": "^3.3.1",
    "tslint": "^5.12.1",
    "vscode-test": "^1.0.0-next.0"
  },
  "dependencies": {
    "fs": "0.0.1-security",
    "path": "^0.12.7"
  }
}
